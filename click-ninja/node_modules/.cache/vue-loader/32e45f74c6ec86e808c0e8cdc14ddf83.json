:"","sourcesContent":["<template>\r\n  <div class=\"main-block\" v-if=\"isShow\" @click=\"stopTimer\">\r\n      click me\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    props: ['delay'],\r\n    data(){\r\n        return{\r\n            isShow: false,\r\n            timer: null,\r\n            totalTime: 0,\r\n        }\r\n    },\r\n    mounted() {\r\n        setTimeout(() => {\r\n            this.isShow = true;\r\n            this.startTimer();\r\n        }, this.delay);\r\n    },\r\n    methods: {\r\n        startTimer(){\r\n            this.timer = setInterval(() => {\r\n                this.totalTime = this.totalTime + 10;\r\n            }, 10);\r\n        },\r\n        stopTimer(){\r\n            clearInterval(this.timer);\r\n            this.$emit(\"end\", this.totalTime);\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\ntemplate{\r\n    display: flex;\r\n    justify-items: center;\r\n}\r\n.main-block{\r\n    border: 1px solid red;\r\n    background-color: aqua;\r\n    margin:40px auto;\r\n    width: 100px;\r\n    height: 100px;\r\n    text-align: center;\r\n}\r\n</style>"]}]}